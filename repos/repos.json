[{
  "kernelSource": {
    "name": "GDSTK-xiaomi10S",
    "repo": "https://github.com/kai855/Xiaomi_Kernel_OpenSource",
    "branch": "alioth-r-oss",
    "device": "Thyme",
    "defconfig": "thyme_user_defconfig"
  },
  "withKernelSU": true,
  "toolchains": [
    {
      {
        "repo": "https://android.googlesource.com/platform/prebuilts/gas/linux-x86",
        "branch": "master",
        "name": "gas",
        "binPath": []
      },
      {
        "repo": "https://gitlab.com/ThankYouMario/android_prebuilts_clang-standalone/",
        "branch": "11",
        "name": "clang",
        "binPath": ["bin"]
      }
    }
  ],
  "ccache": false,
  "params": {
    "ARCH": "arm64",
    "CC": "proton-clang/bin/clang",
    "externalCommand": {        
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "CROSS_COMPILE_COMPAT": "arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
    }
  },
  "AnyKernel3": {
    "use": true,
    "release": true,
    "repo": "https://github.com/easterNday/AnyKernel3/",
    "branch": "master"
  }
}]
