[{
  "kernelSource": {
    "name": "GDSTK-xiaomi10S",
    "repo": "https://github.com/kai855/Xiaomi_Kernel_OpenSource",
    "branch": "alioth-r-oss",
    "device": "Thyme",
    "defconfig": "thyme_user_defconfig"
  },
  "withKernelSU": true,
    "toolchains": [
  {
    "url": "https://android.googlesource.com/platform/prebuilts/clang/host/linux-x86/+archive/refs/heads/android11-release/clang-r383902.tar.gz",
    "name": "clang",
    "binPath": ["./bin"]
  },
  {
    "url": "https://android.googlesource.com/platform/prebuilts/gcc/linux-x86/aarch64/aarch64-linux-android-4.9/+archive/refs/tags/android-12.1.0_r27.tar.gz",
    "name": "gcc",
    "binPath": ["bin"]
  }
    ],
    "ccache": true,
    "params": {
      "ARCH": "arm64",
      "CC": "clang/bin/clang",
      "externalCommand": {
        "CROSS_COMPILE": "aarch64-linux-gnu-",
        "CROSS_COMPILE_ARM32": "arm-linux-gnueabi-",
        "CROSS_COMPILE_COMPAT": "arm-linux-gnueabi-",
        "CLANG_TRIPLE": "aarch64-linux-gnu-"
      }
  },
  "AnyKernel3": {
    "use": true,
    "release": true,
    "repo": "https://github.com/easterNday/AnyKernel3/",
    "branch": "master"
  }
}]
